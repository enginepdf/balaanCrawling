{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [ 
    {
      "name": "client",  
      "image": "enginepdf/multi-client",
      "hostname": "client",   
      "essential": false,
      "memory": 128 
    },
    {
      "name": "server",
      "image": "enginepdf/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "enginepdf/multi-nginx",
      "hostname": "nginx", 
      "essential": true,   
      "portMappings": [
        {
          "hostPort": 80,  
          "containerPort": 80 
        }
      ],
      "links": ["client", "server"],  
      "memory": 128
    }
  ]
}
